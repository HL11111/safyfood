<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.system.mapper.GovernmentStandardMapper">

    <select id="selectStandardById" resultType="com.system.pojo.GovernmentStandard">
        SELECT * FROM government_standard
        where standard_id = #{standardId};
    </select>
</mapper>

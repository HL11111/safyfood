<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.system.mapper.FoodInspectionReportMapper">

    <select id="selectReportById" resultType="com.system.pojo.FoodInspectionReport">
        SELECT * FROM food_inspection_report
        where food_id = #{foodId};
    </select>
</mapper>
